{
  "name": "esnext-quickstart",
  "version": "1.0.0",
  "description": "ESNext quickstart",
  "engines": {
    "node": ">=0.10"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nkbt/esnext-quickstart.git"
  },
  "author": "Nik Butenko <nik@butenko.me> (http://butenko.me/)",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf example",
    "build": "webpack",
    "start": "webpack-dev-server --config webpack.config.development.js",
    "test": "karma start ./karma.conf.js --single-run --reporters nyan",
    "test:dev": "karma start ./karma.conf.js --no-single-run --auto-watch --reporters nyan",
    "test:cov": "COVERAGE=1 karma start ./karma.conf.js --single-run --reporters coverage",
    "test:e2e": "nightwatch-autorun",
    "lint": "eslint --cache .",
    "precommit": "npm run lint -s",
    "prepush": "npm run test -s && npm run test:e2e -s"
  },
  "dependencies": {
    "react": "^0.14.2",
    "react-dom": "^0.14.2"
  },
  "devDependencies": {
    "babel-core": "^6.1.21",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^6.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.1.20",
    "babel-plugin-transform-object-rest-spread": "^6.1.18",
    "babel-polyfill": "^6.1.19",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "css-loader": "^0.23.0",
    "eslint": "^1.9.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.8.0",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.6.2",
    "husky": "^0.10.1",
    "inject-loader": "^2.0.1",
    "isparta-loader": "^1.0.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.15",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-junit-reporter": "^0.3.8",
    "karma-nyan-reporter": "0.2.2",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-webpack": "^1.7.0",
    "lodash": "^3.10.1",
    "mkdirp": "^0.5.1",
    "nightwatch": "^0.8.6",
    "nightwatch-autorun": "^1.0.0",
    "null-loader": "^0.1.1",
    "phantomjs": "^1.9.18",
    "react-addons-test-utils": "^0.14.2",
    "react-hot-loader": "^1.3.0",
    "react-jasmine-matchers": "^2.0.0",
    "rimraf": "^2.4.3",
    "selenium-standalone": "^4.7.1",
    "style-loader": "^0.13.0",
    "tap-xunit": "^1.2.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.6",
    "webpack-dev-server": "^1.12.1"
  }
}
